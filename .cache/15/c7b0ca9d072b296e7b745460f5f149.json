s","original":{"line":295,"column":0},"generated":{"line":295,"column":0}},{"source":"assets/style.css","original":{"line":296,"column":0},"generated":{"line":296,"column":0}},{"source":"assets/style.css","original":{"line":297,"column":0},"generated":{"line":297,"column":0}},{"source":"assets/style.css","original":{"line":298,"column":0},"generated":{"line":298,"column":0}},{"source":"assets/style.css","original":{"line":299,"column":0},"generated":{"line":299,"column":0}},{"source":"assets/style.css","original":{"line":300,"column":0},"generated":{"line":300,"column":0}},{"source":"assets/style.css","original":{"line":301,"column":0},"generated":{"line":301,"column":0}}],"sources":{"assets/style.css":"@import url('https://fonts.googleapis.com/css2?family=Charm:wght@400;700&display=swap');\n@import url('https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,100..900;1,100..900&display=swap');\n\n:root {\n    --font-size-base: 16px;\n    --primary-color: #45474B;\n    --secondary-color: #F5F7F8;\n    --tertiary-color: #379777;\n\n    --border-radius-small: 0.25rem;\n    --border-radius-medium: 0.625rem;\n    --border-radius-circle: 50%;\n    --transition-fast: 0.3s;\n\n    font-size: var(--font-size-base);\n}\n\n* {\n    margin: 0;\n    padding: 0;\n    box-sizing: border-box;\n}\n\nbody {\n    font-family: \"Montserrat\", sans-serif;\n    background-color: var(--secondary-color);\n    width: 100%;\n    height: 100vh;\n}\n\n/* Navbar */\n.navbar {\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    background-color: var(--secondary-color);\n    width: 100%;\n    padding: 0.6rem 1.2rem;\n    border-bottom: 0.0625rem solid rgba(69, 71, 75, 0.2);\n    position: fixed;\n    top: 0;\n    z-index: 1000;\n}\n\n.navbar .logo {\n    font-family: \"Charm\", cursive;\n    font-size: 2rem;\n    font-weight: bold;\n    color: var(--primary-color);\n    font-style: italic;\n    font-weight: 400;\n    margin-left: 1rem;\n}\n\n.navbar .logo:hover {\n    cursor: pointer;\n}\n\n.navbar .logo strong {\n    font-weight: 700;\n    color: #45474b;\n}\n\n.nav-links {\n    display: flex;\n    align-items: center;\n    gap: 0.9375rem;\n    margin-right: 1rem;\n}\n\n/* Botão de Login */\n.login-btn {\n    background-color: black;\n    color: var(--secondary-color);\n    border: none;\n    padding: 0.5rem 1rem;\n    border-radius: var(--border-radius-small);\n    cursor: pointer;\n    transition: background-color var(--transition-fast);\n}\n\n.login-btn:hover {\n    background-color: var(--tertiary-color);\n    color: var(--secondary-color);\n}\n\n/* Link de Cadastro */\n.signup-link {\n    color: var(--primary-color);\n    text-decoration: none;\n    font-weight: bold;\n    transition: color var(--transition-fast);\n}\n\n.signup-link:hover {\n    color: var(--tertiary-color);\n}\n\n/* Profile */\n.profile-container {\n    width: 100%;\n    max-width: 56.25rem;\n    margin: 3.5rem auto;\n    padding: 1.5rem;\n    background-color: var(--secondary-color);\n}\n\n.profile-header {\n    display: flex;\n    align-items: center;\n    padding-bottom: 1.25rem;\n    border-bottom: 1px solid var(--primary-color);\n}\n\n.profile-pic {\n    width: 7.5rem;\n    height: 7.5rem;\n    border-radius: var(--border-radius-circle);\n    margin-right: 1.25rem;\n    border: 3px solid var(--primary-color);\n}\n\n.profile-info h1 {\n    font-size: 1.5rem;\n    color: var(--primary-color);\n    margin-bottom: 0.5rem;\n    font-weight: 600;\n}\n\n.profile-info p {\n    font-size: 1rem;\n    color: var(--primary-color);\n    font-weight: 500;\n}\n\n/* Grid de Imagens */\n.image-grid {\n    display: grid;\n    grid-template-columns: repeat(3, 1fr);\n    gap: 0.4rem;\n    margin-top: 1.25rem;\n}\n\n.image-grid img {\n    width: 100%;\n    height: 16rem;\n    object-fit: cover;\n    transition: transform var(--transition-fast) ease, filter var(--transition-fast) ease;\n}\n\n.image-grid img:hover {\n    transform: scale(1.01);\n    filter: brightness(0.85);\n}\n\n/* Modal */\n.modal {\n    display: none;\n    position: fixed;\n    z-index: 1000;\n    left: 0;\n    top: 0;\n    width: 100%;\n    height: 100%;\n    background-color: rgba(1, 3, 4, 0.66);\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    padding: 1.25rem;\n    text-align: center;\n}\n\n.modal-content {\n    display: block;\n    max-width: 70vh;\n    max-height: 70vh;\n    border-radius: var(--border-radius-medium);\n    margin: 2rem auto 0.5rem;\n}\n\n#caption {\n    background-color: var(--secondary-color);\n    color: var(--primary-color);\n    padding: 1.8rem;\n    border-radius: 0.3125rem;\n    max-width: 69vh;\n    margin: 0 auto;\n    box-sizing: border-box;\n    line-height: 1.5;\n    font-weight: 500;\n}\n\n.close {\n    position: absolute;\n    top: 2.5rem;\n    right: 36rem;\n    color: var(--secondary-color);\n    font-size: 3rem;\n    font-weight: bold;\n    cursor: pointer;\n}\n\n.close:hover,\n.close:focus {\n    color: var(--secondary-color);\n}\n\n/* Responsividade */\n@media (max-width: 480px) {\n\n    .navbar .logo {\n        font-size: 1.5rem;\n    }\n\n    .nav-links {\n        gap: 0.9rem;\n        font-size: 0.75rem;\n    }\n\n    .profile-pic {\n        width: 4rem;\n        height: 4rem;\n        margin-right: 1rem;\n    }\n\n    .profile-info h1 {\n        font-size: 1rem;\n    }\n\n    .profile-info p {\n        font-size: 0.75rem;\n    }\n\n    .image-grid {\n        width: 1vh;\n    }\n\n    .image-grid img {\n        width: 6.5rem;\n        height: 6.5rem;\n    }\n\n    .modal-content {\n        max-width: 80%;\n        max-height: 80vh;\n    }\n\n    .close {\n        right: 1.5rem;\n        font-size: 2rem;\n        top: 2.75rem;\n    }\n\n    #caption {\n        max-width: 80%;\n        font-size: 0.8rem;\n        padding: 1rem;\n        line-height: 1.2;\n    }\n}\n\n@media (min-width: 481px) and (max-width: 868px) {\n    .image-grid {\n        grid-template-columns: repeat(3, 1fr);\n        margin-left: 1rem;\n    }\n\n    .image-grid img {\n        width: 15rem;\n        height: 14rem;\n    }\n\n    .modal-content {\n        margin-top: 1rem;\n        max-width: 60%;\n        max-height: 70vh;\n    }\n\n    .close {\n        font-size: 2.5rem;\n        top: 1.9rem;\n        right: 4rem;\n    }\n\n    #caption {\n        font-size: 1rem;\n        max-width: 60%;\n        padding: 1.4rem;\n        line-height: 1.4;\n    }\n\n    .image-grid img:hover {\n        transform: scale(1.05);\n    }\n\n    .profile-info p {\n        font-size: 0.75rem;\n        color: var(--primary-color);\n        font-weight: 500;\n    }\n}"},"lineCount":301}},"error":null,"hash":"6a076edb4238e888e813df5a03cbd7d4","cacheData":{"env":{}}}